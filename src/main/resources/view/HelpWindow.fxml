<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.stage.Stage?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ScrollPane?>

<fx:root resizable="true" title="Help" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/help_icon.png" />
  </icons>
  <scene>
    <Scene>
        <ScrollPane>
            <VBox alignment="CENTER_LEFT">
                <children>
                    <Label fx:id="helpMethod"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="helpMessage"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="addMethod"  >
                        <VBox.margin>
                            <Insets right="1.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="addMessage"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="listMethod"  >
                        <VBox.margin>
                            <Insets right="1.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="listMessage"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="deleteMethod"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="deleteMessage"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="findMethod"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="findMessage"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="updateMethod"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="updateMessage"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="exitMethod"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>

                    <Label fx:id="exitMessage"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>
                    <Label fx:id="disclaimer" >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>
                    <Label fx:id="url"  >
                        <VBox.margin>
                            <Insets right="5.0" />
                        </VBox.margin>
                    </Label>
                </children>
                <opaqueInsets>
                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
                </opaqueInsets>
                <padding>
                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
                </padding>
            </VBox>
        </ScrollPane>

    </Scene>
  </scene>
</fx:root>
